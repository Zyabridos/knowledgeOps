{
  "name": "game-print-hub-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "migrate:make": "knex migrate:make --knexfile ./knexfile.cjs",
    "migrate:latest": "knex migrate:latest --knexfile ./knexfile.cjs",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.cjs",
    "seed:run": "knex seed:run --knexfile ./knexfile.cjs"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "dotenv": "^16.4.0",
    "fastify": "^5.6.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2",
    "vitest": "^4.0.14"
  }
}
